<!DOCTYPE html>
<html>
  <%- include("partials/head") %>
  <body>
    <%- include("partials/header") %>
    <div class="content">
      <h1><%= title %></h1>
      <% captured.forEach((pokemon) => { %>
      <a class="poke-list" href="/pokedex/captured/<%= pokemon._id %>"
        ><%= pokemon.pokemon.name %></a
      >
      <% }) %>
      <a href="/pokedex/captured/create"
        ><button class="add" type="button">Add Captured Pokemon</button></a
      >
    </div>
  </body>
</html>
